    <sql id="selectFloorOrderVo">
        select id, status, customer_id, supply_id, supply_name, floor_type, floor_spec, floor_number, survey_name_id, survey_name, survey_area, survey_time, survey_img, send_block, send_area, actual_use_block, install_name_id, install_name, install_time, install_area, install_img, install_amount, create_by, create_time, update_by, update_time, price, amount, remark from floor_order
    </sql>

    <select id="selectFloorOrderList" parameterType="FloorOrder" resultMap="FloorOrderResult">
        <include refid="selectFloorOrderVo"/>
        <where>  
            <if test="status != null "> and status = #{status}</if>
            <if test="customerId != null "> and customer_id = #{customerId}</if>
            <if test="supplyId != null "> and supply_id = #{supplyId}</if>
            <if test="floorType != null "> and floor_type = #{floorType}</if>
            <if test="floorSpec != null  and floorSpec != ''"> and floor_spec = #{floorSpec}</if>
            <if test="floorNumber != null  and floorNumber != ''"> and floor_number = #{floorNumber}</if>
        </where>
    </select>
    
    <select id="selectFloorOrderById" parameterType="Long" resultMap="FloorOrderResult">
        <include refid="selectFloorOrderVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertFloorOrder" parameterType="FloorOrder" useGeneratedKeys="true" keyProperty="id">
        insert into floor_order
        <trim prefix="(" suffix=")" suffixOverrides=",">
